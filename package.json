{
  "name": "kaaphal-website",
  "private": true,
  "version": "1.0.0",
  "description": "kaaphal is a website for information and articles",
  "scripts": {
    "dev": "nodemon src/main.ts",
    "build:clean": "rimraf .next",
    "build:ui": "npx next build",
    "build:server": "npx tsc --project tsconfig.server.json && npx babel .next/production-server -d .next/production-server/src --extensions \".js\"",
    "build": "yarn build:clean && yarn build:ui && yarn build:server",
    "start": "cross-env NODE_ENV=production node .next/production-server/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate-gql": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@devoxa/prisma-relay-cursor-connection": "^2.0.2",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@prisma/client": "^2.24.0",
    "@types/passport-facebook": "^2.1.10",
    "antd": "^4.16.0",
    "antd-img-crop": "^3.14.1",
    "apollo-link-token-refresh": "^0.3.2",
    "apollo-server-express": "^2.24.1",
    "apollo-upload-client": "^15.0.0",
    "aws-sdk": "^2.920.0",
    "braft-editor": "^2.3.9",
    "braft-extensions": "^0.1.1",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "file-type": "^16.5.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.5",
    "graphql-upload": "^12.0.0",
    "image-size": "^1.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "mobx": "^6.3.2",
    "mobx-persist-store": "^1.0.3",
    "mobx-react": "^7.2.0",
    "nest-aws-sdk": "^1.1.1",
    "nest-next": "^9.4.0",
    "next": "^10.2.3",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "prisma": "^2.24.0",
    "rc-animate": "^3.1.1",
    "rc-queue-anim": "^1.8.5",
    "rc-texty": "^0.2.0",
    "rc-tween-one": "^2.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lazyload": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sharp": "^0.28.3",
    "slugify": "^1.5.3",
    "uniqid": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^15.6.1",
    "@types/passport-google-oauth20": "^2.0.7",
    "@types/passport-jwt": "^3.0.5",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-lazyload": "^3.1.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "npm": "^7.14.0",
    "postcss": "^8.3.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "tailwindcss": "^2.1.2",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
